extends pug/_layout
block var
  - metas = {}
  - metas.title = 'title'
  - metas.url = 'url'
  - metas.image = 'image'
  - metas.site_name = 'site_name'
  - metas.description = 'description'
block append meta
  +meta(metas)
  include pug/_inc_meta_facebook
  +inc_meta_facebook(metas)
block title
  title ONEMOREDOT.
block body
  include pug/_header
  include pug/_footer